{
  "headers": [
    {
      "source": "/assets/**",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" },
        { "key": "Content-Type", "value": "image/svg+xml" }
      ]
    }
  ],
  "routes": [
    {
      "src": "/assets/.*",
      "dest": "/assets/$1"
    },
    {
      "src": "/.*\\.(js|css|png|jpg|jpeg|svg|ico|woff|woff2|ttf)",
      "status": 200
    },
    {
      "src": "/.*",
      "dest": "/browser/index.html"
    }
  ]
}
